#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>
int main()
{
	float a[10], sum = 0, aver = 0;;
	int i = 0;
	for (i = 0; i < 10; i++)
	{
		scanf("%f", &a[i]);
		sum += a[i];
	}
	aver = sum / 10;
	for (i = 0; i < 10; ++i)
	{
		if (i % 10 == 0)
			printf("\n");//一行显示10个
	}
	printf("\n平均成绩为:>%4.1f\n", aver);
}
