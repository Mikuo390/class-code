#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>
int main()
{
	char c;
	printf("请输入一个字符\n");
	c = getchar();
	if (c < 32)
	{
		printf("这是一个控制字符");
	}
	else if (c >= '0' && c <= '9')
	{
		printf("这是一个数字");
	}
	else if (c >= '0' && c <= '9')
	{
		printf("这是一个数字");
	}
	else if (c >= 'A' && c <= 'Z')
	{
		printf("这是一个大写字母");
	}
	else if (c >= 'a' && c <= 'z')
	{
		printf("这是一个小写字母");
	}
	else
	{
		printf("这是其他字符");
	}
	return 0;
}
